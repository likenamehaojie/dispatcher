		</table>		
	</singleExcel>
    <!-- 导入值班表wk 2015/10/16  end -->

	<!-- 导入带班表  wk 2015/10/16 begin -->
	<singleExcel excelName = "DTDuty_zbb_main_sub" excelTitle="带班表" titleCoordinate="0,0" beginCoordinate="0,3" >
		<table tableName="DTDuty_zbb_main" fields="yearAndMonth,Company,fDate,ReBranch,remark" excelFields="" defaultFields="" conditionFields=""   mrFields="" dateFields="" intFields="" doubleFields="" filterOPTFields="" >
